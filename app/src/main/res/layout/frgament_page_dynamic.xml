<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EAEAEA">

    <include layout="@layout/base_activity_head"
        android:id="@+id/rl_base_head"></include>

    <com.amsu.dategirls.view.LastMsgListView
        android:id="@+id/lv_dynamic_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:divider="@null"
        android:layout_below="@+id/rl_base_head">
    </com.amsu.dategirls.view.LastMsgListView>

    <RelativeLayout
        android:id="@+id/rl_dynamic_add"
        android:layout_width="@dimen/x140"
        android:layout_height="@dimen/x140"
        android:background="@drawable/bg_bt_bigcircle"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/x36"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="@dimen/x56">
        <ImageView
            android:layout_width="@dimen/x52"
            android:layout_height="@dimen/x52"
            android:layout_centerInParent="true"
            android:src="@drawable/jlb_fbxdt"/>
    </RelativeLayout>

</RelativeLayout>